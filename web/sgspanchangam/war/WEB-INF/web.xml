<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">


	<servlet>
		<servlet-name>PanchangamServlet</servlet-name>
		<servlet-class>org.sgsdatta.panchangam.servlets.PanchangamServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PanchangamServlet</servlet-name>
		<url-pattern>/panchangam</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DataLoaderServlet</servlet-name>
		<servlet-class>org.sgsdatta.panchangam.servlets.DataLoaderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DataLoaderServlet</servlet-name>
		<url-pattern>/loadData</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
